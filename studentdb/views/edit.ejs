<ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/studentdb/records">View</a></li>
      <li class="breadcrumb-item  active">Edit</li>
</ol>
<h1 class="display-4">Add new student records</h1>
<form method="POST" action="/studentdb/update/<%= studentdb.id %>">
<div class="form-group">
    <label for="title">Matric.No</label>
    <input type="text" name="title" class="form-control" value="<%= studentdb.title %>">
</div>
<div class="form-group">
    <label for="firstname">Firstname</label>
    <input type="text" name="firstname" class="form-control" value="<%= studentdb.firstname %>">
</div>
<div class="form-group">
    <label for="lastname">Lastname</label>
    <input type="text" name="lastname" class="form-control" value="<%= studentdb.lastname %>">
</div>
<div class="form-group">
    <label for="sex">Sex</label>
    <input type="text" name="sex" class="form-control" value="<%= studentdb.sex %>">
</div>
<div class="form-group">
    <label for="age">Age</label>
    <input type="text" name="age" class="form-control" value="<%= studentdb.age %>">
</div>
<div class="form-group">
    <label for="course">Course</label>
    <input type="text" name="course" class="form-control" value="<%= studentdb.course %>">
</div>
<div class="form-group">
    <label for="level">Level</label>
    <input type="text" name="level" class="form-control" value="<%= studentdb.level %>">
</div>
<div class="form-group">
    <label for="cgpa">CGPA</label>
    <input type="text" name="cgpa" class="form-control" value="<%= studentdb.cgpa %>">
</div>
<div class="form-group">
    <label for="body">Email address</label>
    <textarea name="body" class="form-control"><%= studentdb.body %></textarea>
</div>
<input type="submit" value="create" class="btn btn-primary">
</form>